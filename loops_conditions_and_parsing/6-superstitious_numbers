#!/usr/bin/env bash
# This script displays numbers from 1 to 20, replacing 4, 9, and 17 with superstitious phrases.

# Инициализируем счетчик, начиная с 1
COUNT=1

# Цикл от 1 до 20
while [ $COUNT -le 20 ]
do
    # Используем case для проверки специальных чисел
    case $COUNT in
        # 4-я итерация: '4 ' и 'bad luck from China' на одной строке
        4)
            echo -n "$COUNT "
            echo "bad luck from China"
            ;;
        # 9-я итерация
        9)
            echo -n "$COUNT "
            echo "bad luck from Japan"
            ;;
        # 17-я итерация
        17)
            echo -n "$COUNT "
            echo "bad luck from Italy"
            ;;
        # Для всех остальных чисел
        *)
            echo "$COUNT"
            ;;
    esac
    
    # Увеличиваем счетчик
    COUNT=$((COUNT + 1))
done
