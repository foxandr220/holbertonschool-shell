#!/usr/bin/env bash
# This script displays superstitious numbers from 1 to 20 using while and case.

# Инициализация счетчика с 1
COUNT=1

# Цикл от 1 до 20
while [ $COUNT -le 20 ]
do
    case $COUNT in
        # 4-я итерация: 4 bad luck from China (включая пробел)
        4)
            echo "$COUNT bad luck from China"
            ;;
        # 9-я итерация: 9 bad luck from Japan
        9)
            echo "$COUNT bad luck from Japan"
            ;;
        # 17-я итерация: 17 bad luck from Italy
        17)
            echo "$COUNT bad luck from Italy"
            ;;
        # Все остальные: просто число
        *)
            echo "$COUNT"
            ;;
    esac
    
    # Инкремент
    COUNT=$((COUNT + 1))
done

